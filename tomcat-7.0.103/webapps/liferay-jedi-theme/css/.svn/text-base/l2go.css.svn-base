/* Navigation */

#navigation {
	background: #000;
	border-bottom: 1px solid #333;
	height: 28px;
	min-height: 2.2em;
	font-size: 11px;
	line-height: 1;
	margin: 0;
	overflow: visible;
	padding: 0;
	position: relative;
	width: auto;
	z-index: 10;
}

#navigation ul {
	
}

#navigation li, #navigation ul.child-menu li {
	background: #000;
	background-image: none;
	border: none;
	border-right: 1px solid #333;
	height: auto;
	margin: 0;
	padding: 0;
	display: table-cell;
	float: none;
}

#navigation ul.child-menu li {
	display: block;
}

#navigation li a, #navigation li.selected a, #navigation ul.child-menu > li > a {
	background: none;
	color: #fff;
	font-size: 1.1em;
	font-weight: bold;
	margin: 0;
	padding: 8px 15px;
	text-decoration: none;
	display: table-cell;
}

#navigation li:hover, #navigation li.selected:hover {
	background: #333;
}

#navigation li:hover > a, #navigation ul.child-menu > li:hover > a{
	color: #F17B0D;
	background: inherit;
}

#navigation li:hover > ul.child-menu {
	display: block;
}

#navigation li.selected, #navigation ul li.selected {
	background: #000;
	background-image: none;
}

#navigation ul li.selected.parent-nav-item a{
	color: #F17B0D;
}

#navigation ul.ui-sortable ul.child-menu li.sortable-item > a {
    color: #FFFFFF;
}

#navigation li.selected a:hover {
	padding: 8px 15px;
	height: auto;
}

#navigation li.init a {
	color: #fff;
}

#navigation li a span{
	padding: 0;
	margin: 0;
}

#navigation ul.child-menu {
	background-color: black;
	height: auto;
	width: 250px;
	left: auto;
	top: 29px;
	padding: 0px;
	border: none;
	position: absolute;
	overflow: visible;
	z-index: 79;
	border-left: 1px solid #333;
	border-right: 1px solid #333;
	border-bottom: 1px solid #333;
}

#navigation ul.child-menu li {
	border: none;
}

#navigation ul.child-menu li:hover, #navigation ul.child-menu li.selected {
	background-image: none;
	background: #333;
}

#navigation ul.child-menu li a {
	height: auto;
	width: 230px;
}

#navigation ul li:first-child {
    display: none;
    visibility: hidden;
}

#navigation ul li.selected ul.child-menu li a {
    color: #FFFFFF;
}

