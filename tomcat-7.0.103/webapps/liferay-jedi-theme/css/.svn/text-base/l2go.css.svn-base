/* ---------- Suche ---------- */
#suche {
    background-color: #FFFFFF;
    position: absolute;
    right: 1px;
    top: 20px;
    width: 230px;
    //overflow: hidden;
}

#suche.wide {
	width: 494px;
}

#suche input {
  background: #fff;
  box-shadow: 0px 2px 2px #ccc inset;
  border: none;
  color: #211C19;
  font-size: 13px;
  font-weight: normal;
  margin: 0px 0px 0px 0px;
  padding: 2px 0px 2px 5px;
  height: 16px;
  width: 100%;
  z-index: 333;
  position: relative;
  left: 0px;
  border-radius: 0px;
}

#suche img {
  position: absolute;
  height: 17px;
  right: 101px;
  z-index: 333;
  border-left: 0px solid #c2c2c2;
  border-top: 1px solid #c2c2c2;
  border-bottom: 1px solid #c2c2c2;
}

.uni-form fieldset legend { 
	font-weight: bold;
	color: gray;
	font-size: 120%;
}

#social{
	position: absolute;
	right:260px;
	top:133px;
	z-index:11;
}

#suche > .ui-autocomplete {
	background: #fff;
	margin: 0px;
	padding: 10px 0px;
	left: 0px;
	top: 0px;
	position: relative;
	width: auto;
	border-top: 1px solid #eee;
}

#suche .ui-autocomplete li {
	list-style: none;
	padding: 5px 0px;
	margin: 0px 10px;
}

#suche .ui-autocomplete li.focus {
	background: #ddd;
	cursor: pointer;
}

#suche .ui-autocomplete li a {
	color: #888;
	font-size: 13px;
	font-weight: normal;
}

#suche .ui-autocomplete li a:hover {
	text-decoration: none;
}

#suche .ui-autocomplete li a span.marked{
	color: #000;
}

#suche .ui-autocomplete-even, .results-row td {
	background: #1A1818;
	padding: 5px 10px;
}

#suche .ui-autocomplete-odd, .results-row.alt td {
	background: #222020;
}

#suche .ui-autocomplete-over, .results-row.hover td {
	background: #241202;
}

.fb:hover{
	background: url(../images/l2go/fb_over.png) no-repeat;
}

.tw:hover{
	background: url(../images/l2go/tw_over.png) no-repeat;
}

.ui-tabs {
    border: medium none;
    float: left;
    margin: 0;
    padding: 0;
    width: 648px;
}

.ui-tabs li a, .tabs li span {
	background: #000;
	border-right: 1px #000;
	color: #C2C2C2;
	display: block;
	float: left;
	position: relative;
	text-decoration: none;
	top: 1px;
}

.ie6 .ui-tabs li a, .ie6 .tabs li span {
	background-image: none;
	border-bottom: 3px solid #000;
	padding-bottom: 7px;
}

.ui-tabs li a:hover, .ui-tabs li span:hover {
	background: #202020;
	color: #F17B0D;
}

.ui-tabs li.current a, .ui-tabs li.current span {
	/*background: #000;
	border: 1px solid #000;
	border-bottom: none;*/
	color: #F17B0D;
	font-weight: bold;
}

.ui-tabs li.current:hover, .ui-tabs li.current a:hover {
	background-color: #2E1806;
}

